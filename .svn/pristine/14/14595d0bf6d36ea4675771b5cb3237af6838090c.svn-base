@(user: User)

<div class="menu">
    <div class="item">
        <a href="@routes.Profile.profile(user.id)">
            <span>Информация</span>
        </a>
    </div>
    <div class="item">
        <a href="@routes.Profile.userCycles(user.id)">
            <span>Созданные циклы</span>
        </a>
    </div>
    <div class="item">
        <a href="@routes.Profile.userProjects(user.id)">
            <span>Участие в проектах</span>
        </a>
    </div>
    @if((session.get("email")!=null)&&(session.get("email")==user.email)){
    <div class="item">
        <a href="@routes.Profile.userSettings(user.id)">
            <span>Настройки</span>
        </a>
    </div>
    }
</div>
<div class="clear"/>